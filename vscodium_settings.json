// Place your settings in this file to overwrite the default settings
{
  "files.associations":
    {
    "*.yml": "ansible",
    "Vagrantfile": "ruby",
    },
  "editor.fontFamily": "IBM Plex Mono, Fantasque Sans Mono, Iosevka evilroot, Hack,PragmataPro,Ubuntu Mono",
  "editor.fontLigatures": false,
  "solargraph.transport": "stdio",
  "editor.minimap.enabled": false,
  "typescript.check.tscVersion": false,
  "editor.wordWrap": "on",
  "editor.autoClosingBrackets": "never",
  "editor.renderWhitespace": "all",
  "workbench.iconTheme": "vscode-great-icons",
  "window.zoomLevel": 0,
  "explorer.confirmDelete": false,
  "workbench.colorTheme": "Material Theme Palenight High Contrast",
  "editor.wordWrapColumn": 80,
  "breadcrumbs.enabled": true,
  "explorer.confirmDragAndDrop": false,
  "workbench.startupEditor": "newUntitledFile",
  "sqltools.useNodeRuntime": true,
  "python.formatting.provider": "black",
  "python.linting.flake8Enabled": true,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "python.jediEnabled": true,
  "terraform.indexing": {
    "enabled": true,
    "liveIndexing": false,
    "delay": 500,
    "exclude": [
      ".terraform/**/*",
      "**/.terraform/**/*"
    ]
  },
  "terraform.languageServer": {
    "enabled": false,
    "args": []
  },
  "C_Cpp.updateChannel": "Insiders",
  "liveshare.presence": true,
  "python.linting.pylintEnabled": false,
  "python.linting.enabled": true,
  "python.pythonPath": "/usr/local/opt/python/bin/python3.7",
  "zenMode.centerLayout": false,
  "window.restoreWindows": "none",
  "terminal.integrated.fontFamily": "Victor Mono",
  "terminal.integrated.fontSize": 12,
  "terminal.integrated.rendererType": "experimentalWebgl",
  "files.hotExit": "off",
  "workbench.editor.enablePreview": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "editor.smoothScrolling": true,
  "editor.autoClosingQuotes": "never",
  "peacock.favoriteColors": [
